#define base_dir .
#define num 0.8
#define erTol 1e-7
NavierStokes
{
    WallModel
    {
        #define wm_dir WallModelOutput
        useWallModel                = true
        outputWallModelData         = true
        wallModelOutputDataInterval = 13
        forcingOffset               = 4
        errorTolerance              = $(erTol)
        rayDim                      = 30
        SampleXLocations            = [1.2, 234,42,0.19, 12.3, 2.4]
        outputDir                   = $(base_dir)/$(wm_dir)
        Momentum
        {
            equation = ODE
            underRelaxation = $(num)
        }
        Turbulence
        {
            equation = ODE
            underRelaxation = $(num)
        }
        Energy
        {
            equation = ODE
            underRelaxation = $(num)
        }
    }
}
